<h1>{{ technology.name }}</h1>

<h2>Opdrachten</h2>

{% set posts = collections.all | getPostsByTechnology(technology.key) %}
<ul>
{% for post in posts %}
	<li>
        <a href="#" onclick="addAssignment({{ post.data.key }})">[{{ post.data.key }}]</a> | 
        <a href="{{post.url | url }}">{{post.data.title}}</a>
    </li>
{% endfor %}
</ul>
<input type="text" id="assignments">
<a href="#" onclick="clearAssignments()">clear</a> | 
<a href="#" onclick="copyThis()">copy</a><br>

